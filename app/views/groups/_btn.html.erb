<% if !group.group_exisits?(current_user)  %>
  <td><%= link_to 'Join this group', group_group_users_path(group.id), method: :post, class: "btn btn-success" %></td>
<% else %>
  <td><%= link_to 'Leave this group', group_group_users_path(group.id), method: :delete, class: "btn btn-danger" %></td>
<% end %>